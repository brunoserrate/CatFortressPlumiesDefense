config_version=4

_global_script_classes=[ {
"base": "Node2D",
"class": "SpawnAlliedController",
"language": "GDScript",
"path": "res://scripts/SpawnAlliedController.gd"
}, {
"base": "Node2D",
"class": "SpawnEnemyController",
"language": "GDScript",
"path": "res://scripts/SpawnEnemyController.gd"
} ]
_global_script_class_icons={
"SpawnAlliedController": "",
"SpawnEnemyController": ""
}

[application]

config/name="Cat Fortress: Plumies Defense"
run/main_scene="res://scenes/MainMenu.tscn"
config/icon="res://icon.png"

[autoload]

EventBusSingleton="*res://scripts/EventBusSingleton.gd"
PauseManager="*res://scripts/PauseManager.gd"

[display]

window/stretch/mode="2d"

[global]

Phy=false
view=false
